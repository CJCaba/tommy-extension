{
  "manifest_version": 3,
  "name": "T.O.M.M.Y",
  "description": "Navigate your browser with voice",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts.js"
      ]
    }
  ]
}