{
  "manifest_version": 3,
  "name": "T.O.M.M.Y",
  "description": "Navigate your browser with voice",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScripts.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "react.js",
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
